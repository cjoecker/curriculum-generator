{
	"name": "curriculum-generator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"export:pdf": "tsx src/utils/export-pdf.ts",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"check:all": "npm run lint . && npm run format -- --check . && npm run check-types",
		"fix:all": "npm run lint-fix:all && npm run format:all && npm run check-types",
		"check-types": "tsc --noEmit --incremental",
		"lint-fix:all": "npm run lint -- --fix .",
		"format": "npx prettier",
		"format:all": "npx prettier --write ."
	},
	"dependencies": {
		"date-fns": "^4.1.0",
		"next": "15.3.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-unicorn": "^58.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"puppeteer": "^24.7.0",
		"puppeteer-core": "^24.7.0",
		"tailwindcss": "^4",
		"tsx": "^4.19.3",
		"typescript": "^5"
	}
}
